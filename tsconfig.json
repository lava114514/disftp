{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "noImplicitAny": true,
      "allowJs": false,
      "outDir": "dist",
      "rootDir": "src"
    },
    "include": ["src/**/*"],
  "exclude": ["node_modules", "build/**/*"]
  }